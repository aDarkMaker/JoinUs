{
	"name": "@orangelan/joinus-form",
	"version": "1.0.8",
	"description": "轻量招新报名表单组件 - 支持多端、自定义配置、文件上传",
	"type": "module",
	"main": "./dist/joinus.js",
	"module": "./dist/joinus.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/joinus.mjs",
			"require": "./dist/joinus.js"
		},
		"./style.css": "./dist/theme.css"
	},
	"files": [
		"dist",
		"public/form.json.example",
		"README.md"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc --declaration --emitDeclarationOnly --outDir dist && vite build",
		"build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
		"prepublishOnly": "bun run build",
		"server": "bun run server/index.ts"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/node": "^25.3.0",
		"typescript": "^5.5.0",
		"vite": "^7.3.1",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-dts": "^4.0.0"
	},
	"dependencies": {
		"remixicon": "^4.9.1"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"keywords": [
		"form",
		"recruitment",
		"registration",
		"joinus",
		"招新",
		"报名",
		"表单",
		"file-upload",
		"multipart"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aDarkMaker/JoinUs.git",
		"directory": "package"
	},
	"bugs": {
		"url": "https://github.com/aDarkMaker/JoinUs/issues"
	},
	"homepage": "https://github.com/aDarkMaker/JoinUs/tree/main/package#readme",
	"engines": {
		"bun": ">=1.0.0",
		"node": ">=18.0.0"
	}
}
